        <stimulus id="stimulus60"/>
      </trial>
      <trial id="trial75">
        <answer>button75</answer>
        <screen id="screen"/>
        <stimulus id="stimulus75"/>
      </trial>
      <trial id="trial90">
        <answer>button90</answer>
        <screen id="screen"/>
        <stimulus id="stimulus90"/>
      </trial>
    </trials>
  </procedure>
