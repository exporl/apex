    <parameters>
      <presentations>3</presentations>
      <skip>0</skip>
      <order>random</order>
    </parameters>
    <trials>
      <trial id="trial-90">
        <answer>button-90</answer>
        <screen id="screen"/>
        <stimulus id="stimulus-90"/>
      </trial>
      <trial id="trial-75">
